{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "dusterio/lumen-passport": "^0.2.11",
        "firebase/php-jwt": "^5.0",
        "illuminated/db-profiler": "^5.8",
        "laravel/lumen-framework": "5.8.*",
        "michaelb/lumen-make": "^1.0",
        "vluzrmos/tinker": "^1.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "maxtrunk/lumen-debugbar": "^2.2@dev",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
